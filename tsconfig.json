{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "~/*": [
        "src/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "stores/*": [
        "src/stores/*"
      ]
    },
    "skipLibCheck": true
  },
  "vueCompilerOptions": {
    "target": 3
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue",
  ],
  "types": [
    "element-plus/global"
  ]
}